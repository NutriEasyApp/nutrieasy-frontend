{
  "expo": {
    "name": "Nutrieasy",
    "slug": "nutrieasyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "notification": {
      "icon": "./src/app/assets/images/notificationIcon.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.nutrieasy.app",
      "useNextNotificationsApi": true,
      "icon": "./src/app/assets/images/notificationIcon.png",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
